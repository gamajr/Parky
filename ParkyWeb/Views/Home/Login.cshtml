@model ParkyWeb.Models.User
@{
    ViewData["Title"] = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Login</h1>
<form method="post">
    <input asp-for="Name" type="text" placeholder="User Name" />
    <input asp-for="Password" type="text" placeholder="Password" />
    <button type="submit" value="Submit" >Login</button>
</form>

